        <record id="ir_cron_ereceipt_get_status" model="ir.cron">
            <field name="name">Egypt e-Receipt: Get Status</field>
            <field name="model_id" ref="model_egypt_ereceipt"/>
            <field name="state">code</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="code">model.cron_get_status()</field>
            <field name="active" eval="True"/>
        </record>

        <record id="ir_cron_ereceipt_retry" model="ir.cron">
            <field name="name">Egypt e-Receipt: Retry Failed Submissions</field>
            <field name="model_id" ref="model_egypt_ereceipt"/>
            <field name="state">code</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">15</field>
            <field name="interval_type">minutes</field>
            <field name="code">model.cron_retry_failed_submissions()</field>
            <field name="active" eval="True"/>
        </record>
    </data>
